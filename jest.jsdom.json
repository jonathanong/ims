{
  "testEnvironment": "jsdom",
  "collectCoverageFrom": [
    "**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/__tests__/",
    "/tests/",
    "/test/",
    "/vendor/",
    ".css"
  ],
  "roots": [
    "client",
    "isomorphic"
  ],
  "cacheDirectory": ".cache/jest",
  "transform": {
    "\\.css$": "<rootDir>/node_modules/jest-css-modules",
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  },
  "collectCoverage": true
}

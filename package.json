    "stylelint": "^9.1.3",
    "stylelint-config-css-modules": "^1.2.0",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",

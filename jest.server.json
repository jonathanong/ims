{
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/__tests__/",
    "/tests/",
    "/test/",
    "/vendor/"
  ],
  "roots": [
    "isomorphic",
    "server"
  ],
  "cacheDirectory": ".cache/jest",
  "collectCoverage": true
}

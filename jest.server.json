{
  "cacheDirectory": "node_modules/.cache/jest",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/__tests__/",
    "/tests/",
    "/test/",
    "/vendor/"
  ],
  "roots": [
    "isomorphic",
    "server"
  ],
  "testEnvironment": "node"
}
